{
  "name": "OctoAudit",
  "version": "0.0.1",
  "description": "Add TODO items from issue comments in GitHub and GitHub Enterprise",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": [
      "https://github.com/*/*/pull/*", "https://github.com/*/*/issues/*",
      "*://*.com/*/*/pull/*", "*://*.com/*/*/issues/*",
      "*://*.net/*/*/pull/*", "*://*.net/*/*/issues/*",
      "*://*.org/*/*/pull/*", "*://*.org/*/*/issues/*",
      "*://*.fr/*/*/pull/*", "*://*.fr/*/*/issues/*",
      "*://*/*/*/pull/*", "*://*/*/*/commit/*"
    ],
    "run_at": "document_end",
    "css": ["css/octoaudit.css"],
    "js": [
      "js/vendor/jquery.min.js",
      "js/octoaudit.js"
    ]
  }]
}
